<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.artbycode.spacelet">

    <application
        android:name=".APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.FilterViewActivity"></activity>
        <activity android:name=".Activity.SectionReportActivity" />
        <activity android:name=".Activity.BaseActivity" />
        <activity android:name=".Activity.SingleSectionActivity" />
        <activity android:name=".Activity.ViewSectionsActivity" />
        <activity android:name=".Activity.NewSectionActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>